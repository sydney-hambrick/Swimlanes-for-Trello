{
  "manifest_version": 3,
  "name": "Swimlanes for Trello",
  "description": "Support arranging Trello lists into horizontal swimlanes. To start a new swimlane, just add | or a swimmer emoji to a list's header.",
  "version": "1.7",
  "author": "Jeff Yaus",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [ 
    "*://trello.com/b/*",
  ]
  "web_accessible_resources": [
    {
      "resources": [
    "css/trello-swimlanes.css"
      ],
    "matches": [ 
    "*://trello.com/b/*"
    ]
    }
  ],
  "page_action": {
    "default_title": "Swimlanes for Trello"
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "MacCtrl+Shift+S"
      }
    }
  }
}
